{
  "name": "ezmaster",
  "version": "3.8.2",
  "description": "Administration of docker applications without any IT skills",
  "private": true,
  "scripts": {
    "lint": "eslint --ext=.js,.vue ./ezmaster-api/index.js ./ezmaster-api/routes/ ./ezmaster-api/middlewares/ ./test/ ./ezmaster-api/lib/ ./ezmaster-front/public/src/",
    "insertversion:docker": "sed -i \"s#\\(image: inistcnrs/ezmaster:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose*.yml",    
    "insertversion:index1": "sed -i \"s#\\(version \\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" ezmaster-front/public/index.html",
    "insertversion:index2": "sed -i \"s#\\(version \\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" ezmaster-rp/index.html",
    "insertversion:index3": "sed -i \"s#\\(version \\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" ezmaster-api/public/index.html",
    "preversion": "npm run lint",
    "version": "npm run insertversion:* && git add ezmaster-front/public/index.html ezmaster-api/public/index.html ezmaster-rp/index.html docker-compose*.yml",
    "postversion": "git push && git push --tags",
    "precommit": "npm run lint"
  },
  "keywords": [
    "docker",
    "ihm",
    "gui"
  ],
  "contributors": [
    {
      "name": "St√©phane Gully",
      "url": "https://github.com/kerphi"
    },
    {
      "name": "Nicolas Thouvenin",
      "url": "https://github.com/touv"
    },
    {
      "name": "Nicolas Lamblin",
      "url": "https://github.com/nlamblin"
    }
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^3.7.1",
    "eslint-config-vue": "^2.0.1",
    "eslint-plugin-html": "^1.5.3",
    "eslint-plugin-vue": "^1.0.0",
    "husky": "^0.11.9"
  }
}
